# Production Environment Variables
# Copy this to your production server as /path/to/api/.env

# Database credentials
KA_DB_USER=production_db_user
KA_DB_PASSWORD=strong_production_password

# JWT token secret (32 characters) - MUST match what's in .htaccess
KA_MEMBER_KEY=same_key_as_in_htaccess_file

# GoCardless LIVE credentials
GOCARDLESS_WEBHOOK_SECRET=live_webhook_secret
GOCARDLESS_ACCESS_TOKEN=live_access_token

# Email password
EMAIL_PASSWORD=production_email_password
